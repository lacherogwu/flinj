{
	"name": "flinj",
	"version": "0.4.0",
	"description": "Enhanced REST API library",
	"main": "lib/main.mjs",
	"files": [
		"lib/*"
	],
	"exports": {
		"import": "./lib/main.mjs",
		"require": "./lib/main.cjs"
	},
	"scripts": {
		"prebuild": "rm -rf lib",
		"build": "rollup -c",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"express",
		"framework",
		"sinatra",
		"web",
		"http",
		"rest",
		"restful",
		"router",
		"app",
		"api",
		"open",
		"hapi",
		"application",
		"middleware"
	],
	"author": "LacheRo",
	"repository": "lacherogwu/flinj",
	"bugs": {
		"url": "https://github.com/lacherogwu/flinj/issues"
	},
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"chalk": "^5.0.1",
		"fast-glob": "^3.2.11"
	},
	"devDependencies": {
		"rollup": "^2.72.0"
	}
}
