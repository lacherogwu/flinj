{
	"name": "flinj",
	"version": "0.1.1",
	"description": "Easy REST API library",
	"main": "lib/main.mjs",
	"files": [
		"lib/*"
	],
	"exports": {
		"import": "./lib/main.mjs"
	},
	"scripts": {
		"prebuild": "rm -rf lib",
		"build": "rollup -c",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"express",
		"framework",
		"sinatra",
		"web",
		"http",
		"rest",
		"restful",
		"router",
		"app",
		"api",
		"open",
		"hapi",
		"application",
		"middleware"
	],
	"author": "LacheRo",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"fast-glob": "^3.2.11"
	},
	"devDependencies": {
		"rollup": "^2.72.0",
		"rollup-plugin-terser": "^7.0.2"
	}
}
